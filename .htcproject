<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<hightec_project_settings eclipse_build_date="0000.00">
                      
  
  
  
  
  
  
  
  
  
  <project_build_properties>
                                            
    
    
    
    
    
    
    
    
    
    <pre_post_build_streps>
      <pre_build_step/>
      <post_build_step>
        <build_step active="true" command="${HtcActiveToolchain}/bin/${HtcArchToolPref}-objcopy" description="The tool objcopy supports different output formats. To generate an Intel Hex format, you can use this post build step" errRedirection="${ProjDirPath}/${ConfigName}/hexError.txt" errRedirectionAppend="false" errRedirectionEnabled="false" name="Generate hex" outRedirection="${ProjDirPath}/${ConfigName}/hex.txt" outRedirectionAppend="false" outRedirectionEnabled="true" position="0">
          <argument value="-O"/>
          <argument value="ihex"/>
          <argument value="${BuildArtifactFileBaseName}.elf"/>
          <argument value="${BuildArtifactFileBaseName}.hex"/>
        </build_step>
        <build_step active="true" command="${HtcActiveToolchain}/bin/${HtcArchToolPref}-objdump" description="The tool objdump provides the functionality to create disassemble of the elf file" errRedirection="${ProjDirPath}/${ConfigName}/disassembleError.txt" errRedirectionAppend="false" errRedirectionEnabled="false" name="Create disassemble" outRedirection="${ProjDirPath}/${ConfigName}/disassemble.txt" outRedirectionAppend="false" outRedirectionEnabled="true" position="1">
          <argument value="-d"/>
          <argument value="${BuildArtifactFileBaseName}.elf"/>
        </build_step>
        <build_step active="true" command="${HtcActiveToolchain}/bin/${HtcArchToolPref}-readelf" description="The tool readelf provides the functionality to list all section headers" errRedirection="${ProjDirPath}/${ConfigName}/sectionHeadersError.txt" errRedirectionAppend="false" errRedirectionEnabled="false" name="Section headers" outRedirection="${ProjDirPath}/${ConfigName}/section_headers.txt" outRedirectionAppend="false" outRedirectionEnabled="true" position="2">
          <argument value="-S"/>
          <argument value="${BuildArtifactFileBaseName}.elf"/>
        </build_step>
        <build_step active="true" command="${HtcActiveToolchain}/bin/${HtcArchToolPref}-size" description="The size of all output sections (e.g. code section .text) can be extracted with this post build step" errRedirection="${ProjDirPath}/${ConfigName}/sizeError.txt" errRedirectionAppend="false" errRedirectionEnabled="false" name="Size" outRedirection="${ProjDirPath}/${ConfigName}/size.txt" outRedirectionAppend="false" outRedirectionEnabled="true" position="3">
          <argument value="-A"/>
          <argument value="${BuildArtifactFileBaseName}.elf"/>
        </build_step>
      </post_build_step>
    </pre_post_build_streps>
                                          
  
  
  
  
  
  
  
  
  
  </project_build_properties>
                    









</hightec_project_settings>
